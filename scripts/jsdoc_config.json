{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": [
            "./src/Linen.js",
            "./src/Model.js",
            "./src/Arc.js",
            "./src/Image.js",
            "./src/Line.js",
            "./src/Rectangle.js",
            "./src/Text.js",
            "./src/Canvas.js",
            "./src/Path.js"
        ]
    },
    "sourceType": "module",
    "opts": {
        "template": "node_modules/docdash",
        "destination": "docs/",
        "tutorials": "./tutorials"
    },
    "docdash": {
        "sectionOrder": [
            "Tutorials",
            "Classes",
            "Modules",
            "Externals",
            "Events",
            "Namespaces",
            "Mixins",
            "Interfaces"
        ],
        "sort": false,
        "private": false,
        "search": true,
        "menu": {
            "GitHub": {
                "href": "https://github.com/brianwendt/linen",
                "target": "_blank",
                "class": "menu-item",
                "id": "website_link"
            }
        }
    }
}